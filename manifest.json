{
    "manifest_version": 3,
    "name": "Jira Ticket Turbo (JTT)",
    "version": "1.0.0",
    "description": "Board Hopper and Ticket Popper - Your Jira Jetpack!",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
      }
    },
    "icons": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  }
  